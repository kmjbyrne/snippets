{
    "indent_char": " ",
    "css": {
        "type": ["css", "scss"],
        "indent_size": 4,
        "end_with_newline": true,
        "preserve_newlines": false,
        "wrap_line_length": 80
    },
    "html": {
        "type": ["htm", "html"],
        "indent_size": 4,
        "end_with_newline": true,
        "extra_liners": [],
        "preserve_newlines": false,
        "wrap_line_length": 120
    },
    "js": {
        "type": ["javascript", "json"],
        "indent_size": 4,
        "jslint_happy": true,
        "brace_style": "collapse",
        "end_with_newline": true,
        "newline_between_rules": true,
        "space_after_anon_function": true,
        "indent_handlebars": true
    },
    "indent_size": 4,
    "end_with_newline": true,
    "extra_liners": false,
    "preserve_newlines": false,
    "wrap_line_length": 80,
    "eol": "\n"
}
